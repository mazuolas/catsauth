<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <% if current_user %>
    <h1><%= current_user.user_name %> is logged in!</h1>
    <%= button_to "Log out",sessions_url, method:'delete' %>
    <% else %>
    <%= button_to "Log in dawg",new_sessions_url, method:'get' %>
  <% end %>
</head>
<body>
  <% if flash[:errors] %>
    <i><h1 style="color:red"><%= flash[:errors] %></h1></i>
  <% end %>
<%= yield %>

</body>
</html>
